@model ProjectTerm

@{
    ViewBag.Title = "Delete Project Term";
    ViewBag.SubPagetitle = "Are you sure you want to delete this?";
    ViewBag.NavigationKey = "Project";
}

@using (Html.BeginForm()) {
	@Html.AntiForgeryToken()
    
    <fieldset class="display-form">
	    <legend>ProjectTerm</legend>
	    <ul>

		    <li>
			    <div class="display-label">Term</div>
			    <div class="display-details">@Model.Term</div>
		    </li>

		    <li>
			    <div class="display-label">Definition</div>
			    <div class="display-details">@Model.Definition</div>
		    </li>

		    <li>
			    <div class="display-label">Src</div>
			    <div class="display-details">@Model.Src</div>
		    </li>
		
		    <li>
			    @* For the clear and submit buttons an empty label is required. Example: <label>&nbsp;</label> *@
			    <div class="display-label">&nbsp;</div>
			    <div class="display-details">
			    <input class="button" type="submit" value="Delete" /> | @Html.ActionLink("Cancel", "Index", new { id=Model.Project.Id })
			    </div>
		    </li>
	    </ul>
    </fieldset>
}




