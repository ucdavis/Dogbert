<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">

  <class name="Dogbert.Core.Domain.ChangeLog, Dogbert.Core" table="ChangeLogs" >
    <id name="Id" column="id" unsaved-value="0">
      <generator class="identity" />
    </id>
    <property name="Reason" column="Reason"></property>
    <property name="RequestedBy" column="RequestedBy"></property>
    <property name="Change" column="Change"></property>
    <property name="Created" column="Created"></property>
    <property name="Updated" column="Updated"></property>
    <many-to-one name="Project" class="Dogbert.Core.Domain.Project, Dogbert.Core" column="ProjectID" cascade="none" />

  </class>

</hibernate-mapping>