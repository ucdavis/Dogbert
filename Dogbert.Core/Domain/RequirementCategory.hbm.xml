<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="Dogbert.Core" namespace="Dogbert.Core.Domain">
  <class
    name="RequirementCategory"
    table="RequirementCategories" >
    <id name="Id" column="ID" unsaved-value="0" >
      <generator class="identity" />
    </id>

    <property name="Name" column="Name" />
    <property name="IsActive" column="IsActive" />
    <!-- 
    <property name="ProjectID" column="ProjectID" />
    -->

    <many-to-one name="Project" class="Project" column="ProjectID" cascade="none" />


  </class>
</hibernate-mapping>